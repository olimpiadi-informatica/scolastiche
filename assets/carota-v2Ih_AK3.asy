picture drawing(real k = 1, pen bg = rgb("f36d21"), pen fg = black) {
    picture pic;
    path p;
    pen bord = black+11.25*k;
    p = (-9, 143) .. controls (2, 162) and (17, 184) .. (28, 198) .. controls (28, 202) and (27, 206) .. (27, 211) .. controls (23, 231) and (6, 246) .. (-12, 243) .. controls (-30, 240) and (-42, 221) .. (-38, 200) .. controls (-37, 191) and (-30, 170) .. (-22, 149) .. controls (-18, 148) and (-13, 146) .. (-9, 143) -- cycle;
    filldraw(pic, p, rgb("79ad14"), bord);
    p = (18, 116) .. controls (37, 127) and (59, 142) .. (73, 153) .. controls (77, 153) and (81, 152) .. (86, 152) .. controls (106, 148) and (121, 131) .. (118, 113) .. controls (115, 95) and (96, 84) .. (75, 87) .. controls (66, 88) and (45, 95) .. (24, 103) .. controls (23, 107) and (21, 112) .. (18, 116) -- cycle;
    filldraw(pic, p, rgb("79ad14"), bord);
    p = (6, 131) .. controls (11, 127) and (15, 122) .. (18, 116) .. controls (43, 129) and (74, 152) .. (83, 161) .. controls (98, 176) and (99, 198) .. (86, 211) .. controls (74, 224) and (51, 222) .. (36, 208) .. controls (27, 199) and (5, 168) .. (-9, 143) .. controls (-3, 140) and (2, 136) .. (6, 131) -- cycle;
    filldraw(pic, p, rgb("a5ce34"), bord);
    p = (6, 131) .. controls (-25, 162) and (-76, 160) .. (-108, 127) .. controls (-158, 77) and (-273, -74) .. (-236, -111) .. controls (-199, -148) and (-48, -33) .. (2, 17) .. controls (35, 50) and (37, 100) .. (6, 131) -- cycle;
    fill(pic, p, bg);
    p = (6, 131) .. controls (19, 102) and (12, 65) .. (-13, 40) .. controls (-60, -7) and (-196, -110) .. (-243, -93) .. controls (-243, -101) and (-241, -107) .. (-236, -111) .. controls (-199, -148) and (-48, -33) .. (2, 17) .. controls (35, 49) and (37, 100) .. (6, 131) -- cycle;
    fill(pic, p, 0.85*bg);
    bord = fg+11.25*k;
    p = (-4, 10) .. controls (-4, 10) and (-34, 20) .. (-64, 50);
    draw(pic, p, bord);
    p = (-123, 18) .. controls (-123, 18) and (-142, 35) .. (-162, 63);
    draw(pic, p, bord);
    p = (-46, -25) .. controls (-46, -25) and (-71, -18) .. (-87, -2);
    draw(pic, p, bord);
    p = (-79, -49) .. controls (-79, -49) and (-105, -44) .. (-132, -17);
    draw(pic, p, bord);
    p = (-201, 4) .. controls (-201, 4) and (-190, -17) .. (-168, -35);
    draw(pic, p, bord);
    p = (-131, 102) .. controls (-131, 102) and (-125, 84) .. (-108, 67);
    draw(pic, p, bord);
    p = (-195, -80) .. controls (-195, -80) and (-181, -96) .. (-164, -103);
    draw(pic, p, bord);
    bord = black+11.25*k;
    p = (52, 177) -- (6, 131);
    draw(pic, p, bord);
    p = (-4, 195) -- (2, 161);
    draw(pic, p, bord);
    p = (70, 121) -- (36, 127);
    draw(pic, p, bord);
    p = (6, 131) .. controls (-25, 162) and (-76, 160) .. (-108, 127) .. controls (-158, 77) and (-273, -74) .. (-236, -111) .. controls (-199, -148) and (-48, -33) .. (2, 17) .. controls (35, 50) and (37, 100) .. (6, 131) -- cycle;
    draw(pic, p, bord);
    return scale(k/32)*pic;
}
